{
  "expo": {
    "name": "halloween-challenge",
    "slug": "halloween-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/rm_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/rm_splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#18173f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/rm_icon.png",
        "backgroundColor": "#97CE4C"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
